    "files.exclude": {
        "**/build/**": true,
        "**/dist/**": true,
        "**/nbproject/private/**": true,
        "**/.classpath": true,
        "**/.project": true,
        "**/.settings/**": true
    },
    "files.watcherExclude": {
        "**/build/**": true,
        "**/dist/**": true
    },
    "java.format.settings.url": "",
    "java.format.settings.profile": "",
    "editor.formatOnSave": true,
    "editor.codeActionsOnSave": {
        "source.organizeImports": "explicit"
    },
    "java.completion.importOrder": [
        "java",
        "javax",
        "org",
        "com"
    ],
    "java.sources.organizeImports.starThreshold": 99,
    "java.sources.organizeImports.staticStarThreshold": 99,
    "xml.validation.enabled": true,
    "xml.validation.namespaces.enabled": "onDemand",
    "html.format.indentInnerHtml": true,
    "css.validate": true,
    "javascript.validate.enable": true,
    "files.associations": {
        "*.jsp": "jsp",
        "*.jspf": "jsp"
    },
    "emmet.includeLanguages": {
        "jsp": "html"
    },
    "emmet.triggerExpansionOnTab": true,
    "jsp.syntax.highlighting": true,
    "jsp.format.enable": true,
    "jsp.completion.enabled": true,
    "jsp.validation.enabled": true,
    "html.suggest.html5": true,
    "html.format.wrapLineLength": 120,
    "html.format.indentInnerHtml": true,
    "html.format.preserveNewLines": true
}