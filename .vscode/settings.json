{
    "java.home": "/usr/lib/jvm/java-8-openjdk-arm64",
    "java.configuration.runtimes": [
        {
            "name": "JavaSE-1.8",
            "path": "/usr/lib/jvm/java-8-openjdk-arm64"
        }
    ],
    "java.compile.nullAnalysis.mode": "automatic",
    "java.configuration.updateBuildConfiguration": "automatic",
    "java.import.gradle.enabled": false,
    "java.import.maven.enabled": false,
    "java.import.ant.enabled": true,
    "java.project.sourcePaths": [
        "HotelReservation-ejb/src/java",
        "HotelReservation-war/src/java"
    ],
    "java.project.outputPath": "build/classes",
    "java.project.referencedLibraries": [
        "lib/**/*.jar",
        "/opt/glassfish4/glassfish/modules/javax.persistence.jar",
        "/opt/glassfish4/glassfish/modules/ejb-api.jar",
        "/opt/glassfish4/glassfish/modules/javax.ejb-api.jar",
        "/opt/glassfish4/glassfish/modules/javax.servlet-api.jar",
        "/opt/glassfish4/glassfish/modules/javax.servlet.jsp-api.jar",
        "/opt/glassfish4/glassfish/modules/javax.servlet.jsp.jstl-api.jar",
        "/opt/glassfish4/glassfish/modules/javax.ws.rs-api.jar",
        "/opt/glassfish4/glassfish/modules/javax.validation-api.jar",
        "/opt/glassfish4/glassfish/modules/bean-validator.jar",
        "/opt/glassfish4/glassfish/modules/endorsed/jaxb-api.jar",
        "/opt/glassfish4/glassfish/modules/javax.xml.ws-api.jar",
        "/opt/glassfish4/glassfish/modules/javax.jws-api.jar",
        "/opt/glassfish4/glassfish/lib/mysql-connector-java-8.0.16.jar"
